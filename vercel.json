{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "api/main.py": {
      "maxDuration": 300
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": true,
      "development": true
    }
  }
} 